{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    // "linebreak-style": ["error", "windows"],
    "react/prop-types": 0,
    "require-jsdoc": 0,
    "max-len": 0,
    "react/display-name": "off",
    "react/no-unescaped-entities": "off",
    "no-unused-vars": "off"
  }
}
